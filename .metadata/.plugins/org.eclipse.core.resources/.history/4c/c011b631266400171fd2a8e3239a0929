package javaSyntax;

public class Bitwise {
	public static void main(String[] args){
		int x = 456;
		System.out.println(Integer.toBinaryString(x));
		int count = 5;
		for(int i=0;i<count;i++) x >>=1;
		System.out.println(x);
		System.out.println(Integer.toBinaryString(x));
		x = 456;
		System.out.println(Integer.toBinaryString(x));
		for(int i=0;i<count;i++) x >>>=1;
		System.out.println(x);
		System.out.println(Integer.toBinaryString(x));
		
	}

}
